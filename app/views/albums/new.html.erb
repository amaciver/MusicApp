<form class="" action="<%=albums_url%>" method="post">
  <%= auth_token %>
  <label for="Title">Title</label>
  <input id="Title" type="text" name="album[title]" value="">

  <label for="Band">Band/Artist</label>
  <select id="Band" class="" name="album[band_id]">
    <% Band.all.each do |band| %>
    <option value="<%= band.id %>"><%= band.name %></option>
    <% end %>
  </select>

  <input type="submit" name="" value="Create Album">
</form>
